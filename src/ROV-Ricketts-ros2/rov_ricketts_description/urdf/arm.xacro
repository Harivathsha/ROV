<?xml version="1.0" encoding="utf-8"?>

<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="arm_macro" params="name *origin">
    <joint name="${name}_base_joint" type="revolute">
      <xacro:insert_block name="origin"/>
      <parent link="base_link" />
      <child link="${name}_base" />
      <axis xyz="0 0 1" />
      <limit
        lower="-0.96"
        upper="3.14"
        effort="20"
        velocity="10" />
      <dynamics damping="0.1" friction="0.3"/>
    </joint>

    <link
      name="${name}_base">
      <inertial>
        <origin
          xyz="0.01134495 -0.00080126  0.07665164"
          rpy="0 0 0" />
        <mass
          value="12.163342453968937" />
        <inertia
          ixx="0.05081885"
          ixy="-0.00077462"
          ixz="-0.00807678"
          iyy="0.05298367"
          iyz="0.00259261"
          izz="0.05370248" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="file://$(find rov_ricketts_description)/meshes/arm_base.stl" />
        </geometry>
        <material
          name="">
          <color
            rgba="0.5 0.5 0.5 1" />
        </material>
      </visual>
      <collision>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
          <mesh
            filename="file://$(find rov_ricketts_description)/meshes/arm_base.stl" />
        </geometry>
      </collision>
    </link>
    
