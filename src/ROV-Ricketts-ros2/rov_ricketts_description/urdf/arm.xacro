      </collision>
    </link>

    <joint name="${name}_link4_link5" type="continuous">
      <origin
          xyz="0.0 -0.000111 -0.070007"
          rpy="${pi} 0 0" />
      <parent link="${name}_link4" />
      <child link="${name}_link5" />
      <axis xyz="0  0 1" />
      <limit
        effort="20"
        velocity="10" />
      <dynamics damping="0.1" friction="0.3"/>
