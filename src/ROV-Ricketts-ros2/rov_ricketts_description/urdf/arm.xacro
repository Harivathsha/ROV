          rpy="0 0 0" />
        <mass
          value="0.7601912208661399" />
        <inertia
          ixx="1.11087526E-03"
          ixy="-9.17650911E-07"
          ixz="-9.18706375E-06"
          iyy="1.20506655E-03"
          iyz="-1.54884566E-04"
          izz="2.32928780E-04" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
            filename="file://$(find rov_ricketts_description)/meshes/grip_claw.stl" />
