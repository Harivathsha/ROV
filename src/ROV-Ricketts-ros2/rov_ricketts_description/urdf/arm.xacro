    <joint name="${name}_hand_tool" type="fixed">
      <origin
          xyz=" 0 0  0.271942"
          rpy="0 0 ${pi}" />
      <parent link="${name}_link5" />
      <child link="${name}_tool" />
    </joint>

    <link name="${name}_tool" />

    
        <ros2_control name="GazeboSystem" type="system">
            <hardware>
                <plugin>gz_ros2_control/GazeboSimSystem</plugin>
            </hardware>
            
    <joint name="${name}_base_joint">
      <command_interface name="position">
        <param name="min">-2</param>
        <param name="max">2</param>
      </command_interface>
      <state_interface name="position">
        <param name="initial_value">0.0</param>
      </state_interface>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="${name}_base_link1">
      <command_interface name="position">
        <param name="min">-2</param>
        <param name="max">2</param>
      </command_interface>
      <state_interface name="position">
        <param name="initial_value">0.0</param>
      </state_interface>
      <state_interface name="velocity"/>
      <state_interface name="effort"/>
    </joint>
    <joint name="${name}_link1_link2">
