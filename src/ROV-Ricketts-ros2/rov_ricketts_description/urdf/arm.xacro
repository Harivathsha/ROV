          ixy="1.47362354E-08"
          ixz="-3.64355893E-08"
          iyy="1.24863733E-02"
          iyz="6.31833371E-08"
          izz="8.03611637e-03" />
      </inertial>
      <visual>
        <origin
          xyz="0 0 0"
          rpy="0 0 0" />
        <geometry>
          <mesh
