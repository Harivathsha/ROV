  </joint>
  <link name="wrist_link">
    <visual>
      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/DAE/wrist/wrist.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/STL/wrist/wrist.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0"/>
      <mass value="0.085"/>
      <inertia ixx="0.00003082" ixy="0.0000000191" ixz="0.0000000023" iyy="0.00002822" iyz="0.0000025481" izz="0.00003152"/>
    </inertial>
    <surface>
      <friction>
        <ode>
          <mu1>1.2</mu1>
          <mu2>1.2</mu2>
          <kp>1e15</kp>
