      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/DAE/finger/finger.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/STL/finger/finger.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0 0.0 0.0"/>
      <mass value="0.016"/>
      <inertia ixx="0.00009" ixy="0.0" ixz="0.0" iyy="0.00009" iyz="0.0" izz="0.00002"/>
