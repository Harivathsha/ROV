  </joint>
  <link name="lower_arm_link">
    <visual>
      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/DAE/lower_arm/lower_arm.dae"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="package://rov_ricketts_description/meshes/STL/lower_arm/lower_arm.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0.0374395000 0.0052225200 0"/>
      <mass value="0.220991"/>
      <inertia ixx="0.00006369" ixy="-0.00002292" ixz="0.0" iyy="0.0001677" iyz="0.0" izz="0.0001834"/>
    </inertial>
    <surface>
      <friction>
        <ode>
          <mu1>1.2</mu1>
          <mu2>1.2</mu2>
          <kp>1e15</kp>
